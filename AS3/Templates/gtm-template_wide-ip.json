{
    "gtm-tenant_testapplication": {
        "class": "Tenant",
        "remark": "App Name: testapplication; Domain: gslbext.example.com",
        "gtm-app_testapplication": {
            "class": "Application",
            "gtm-domain_testapplication": {
                "class": "GSLB_Domain",
                "domainName": "testapplication.gslbext.example.com",
                "aliases": ["testalias.gslbext.example.com"],
                "resourceRecordType": "A",
                "poolLbMode": "round-robin",
                "pools": [{ "use": "gtm-pool_testapplication" }]
            },
            "gtm-pool_testapplication": {
               "class": "GSLB_Pool",
               "resourceRecordType": "A", 
               "lbModePreferred": "round-robin",
                "lbModeAlternate": "round-robin",
                "lbModeFallback": "return-to-dns",
                "members": [{
                        "server": {"use": "/Common/Shared/server_site1-dmz-f5"},
                        "virtualServer": "vs_testapplication_443"},{
                        "server": {"use": "/Common/Shared/server_site3-dmz-f5"},
                        "virtualServer": "vs_testapplication_443"
                }]
            }
        }
    },
    "class": "ADC",
    "schemaVersion": "3.6.0",
    "id": "testapplication"
}
