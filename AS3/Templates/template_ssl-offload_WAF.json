            "securityLogProfiles": [
               {
                  "bigip": "/Common/Local_WAF_Logging"
               },
               {
                  "bigip": "/Common/Shared/telemetry_asm_security_log_profile"
               }
            ]
         },
         "vs_testapplication_http": {
            "class": "Service_HTTP",
            "profileHTTP": {
               "bigip": "/Common/Shared/as3_http"
            },
            "virtualPort": 80,
            "virtualAddresses": [
               "x.x.x.x"
            ],
            "snat": "auto",
            "profileMultiplex": {
               "bigip": "/Common/oneconnect"
            },
            "persistenceMethods": [
               "cookie",
               "source-address"
            ],
            "iRules": [
               "irule_testapplication_redirect"
            ]
         },
         "pool_testapplication_80": {
            "class": "Pool",
            "monitors": [
               {
                  "use": "monitor_testapplication_http"
               }
            ],
            "members": [
               {
                  "servicePort": 80,
                  "serverAddresses": [
                     "y.y.y.y",
                     "z.z.z.z"
                  ]
               }
            ]
         },
         "monitor_testapplication_http": {
            "class": "Monitor",
            "monitorType": "http",
            "send": "GET /adfs/probe HTTP/1.1\r\nHost: \r\n\r\n",
            "receive": "HTTP/1.1 200 Ok",
            "interval": 5,
            "timeout": 16,
            "targetPort": 80
         },
         "irule_testapplication_redirect": {
            "class": "iRule",
            "iRule": "when HTTP_REQUEST {\n   HTTP::redirect https://[getfield [HTTP::host] \":\" 1][HTTP::uri]\n }"
         },
         "dos_testapplication": {
            "class": "DOS_Profile",
            "application": {
